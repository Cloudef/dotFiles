Xcursor.theme: AnArchCursor-26

!URxvt Settings {{{
URxvt*termName:rxvt-256color

URxvt*borderLess:true
URxvt*externalBorder:0
URxvt*internalBorder:0

URxvt*perl-ext-common:default,-tabbed,matcher,-selection-popup,-option-popupi

URxvt*visualBell:false
URxvt*cursorBlink:true
URxvt*highlightSelection:true
URxvt*buffered:true
URxvt*saveLines:1500
cutchars::@-./_~?&=%+#'"'()*,;<=>[]^{|}
URxvt*iconFile: /home/jari/.icons/ACYL_Icon_Theme_0.9.2/scalable/apps/terminal.svg

URxvt*eightBitInput:true
URxvt*inputMethod:uim
URxvt*preeditType:OverTheSpot,root
URxvt*multichar_encoding:utf-8
URxvt*imLocale:fi_FI.UTF-8

modifier:meta
!alt, meta, hyper, super, mod1..5
!}}}

!Scroll and mousewheel {{{
XTerm*jumpScroll:true
XTerm*multiScroll:true
URxvt*jumpScroll:true
URxvt*skipScroll:true
URxvt*scrollBar:false
URxvt*scrollBar_right:false
URxvt*scrollstyle:plain
URxvt*secondaryScroll:true
URxvt*scrollTtyOutput:false
URxvt*scrollWithBuffer:true
URxvt*scrollTtyKeypress:true
URXvt*mouseWheelScrollPage:false
!}}}

!URxvt Fonts {{{
URxvt*font:  xft:erusfont
!}}}

!URxvt Color Settings {{{
#include "/home/jari/.color_schemes/Taters"

URxvt*borderColor:        #151515
URxvt*colorBD:            #ffffff
URxvt*cursorColor:        #ff8939
URxvt*cursorColor2:       #151515

! --- Clickable links --- !
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: opera
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

! }}}
