Xcursor.theme: X-Steel-GRAY

!XFT {{{
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hinting: true
Xft.hintstyle:  hintslight
Xft.antialias: true
Xft.rgba: rgb
Xft.dpi: 95
!}}}

! Input
preeditType:OverTheSpot,Root

! URxvt Settings {{{
URxvt*termName:rxvt-unicode-256color
URxvt.letterSpace:0
URxvt*borderLess:false
URxvt*perl-ext-common:default,-tabbed,matcher,-selection-popup,-option-popupi

URxvt*visualBell:false
URxvt*cursorBlink:true
URxvt*highlightSelection:true
URxvt*buffered:true
URxvt*saveLines:1500
URxvt*cutchars:@-./_~?&=%+#'"'()*,;<=>[]^{|}

! rxvt-unicode
URxvt.urgentOnBell:  true

! xterm
XTerm*bellIsUrgent:  true

URxvt.perl-ext-common:  default,url-select,keyboard-select
URxvt.keysym.M-u:       perl:url-select:select_next
URxvt.urlButton:        0
URxvt.urlLauncher:      dwb
URxvt.underlineURLs:    true
URxvt.keysym.M-Down:    perl:keyboard-select:activate
URxvt.keysym.M-Up:      perl:keyboard-select:search

! ctrl keys
URxvt.keysym.Control-Up:    \033[1;5A
URxvt.keysym.Control-Down:  \033[1;5B
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
!}}}

! Scroll and mousewheel {{{
XTerm*jumpScroll:true
XTerm*multiScroll:true
URxvt*jumpScroll:true
URxvt*skipScroll:true
URxvt*scrollBar:false
URxvt*scrollBar_right:false
URxvt*scrollstyle:plain
URxvt*secondaryScroll:true
URxvt*scrollTtyOutput:false
URxvt*scrollWithBuffer:true
URxvt*scrollTtyKeypress:true
URXvt*mouseWheelScrollPage:false
!}}}

! URxvt Fonts {{{
URxvt.keysym.C-plus: command:\033]50;xft:DejaVu Sans Mono:pixelsize=16,xft:IPAMonaGothic:antialias=false,xft:Symbola\007
URxvt.keysym.C-minus: command:\033]50;xft:erusfont,xft:IPAMonaGothic:antialias=false,xft:Symbola\007
URxvt.font:xft:erusfont,xft:IPAMonaGothic:antialias=false,xft:Symbola
!}}}

! URxvt Color Settings {{{
#include "/home/jari/.Xcolors/Colorshot"

URxvt*depth: 24
!URxvt*background:rgb:1000/1000/1000/cccc

URxvt*colorBD:            #ffffff
URxvt*cursorColor:        #ff8939
URxvt*cursorColor2:       #151515
! }}}

xrenoise.use_xcursors: 1
xrenoise.display: :0.0
xrenoise.display_resource: 0x00,0x15,0xE3,0x1E,0xC1,0x60,0x89,0x43
xrenoise.executable_path: /usr/bin/renoise
